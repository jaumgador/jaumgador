import java.util.arraylist;
// importamosa classe arraylist para poder fazer uso do vetor e armazenar elementos
public class doWhile {
/* metodo principal de classe*/
 public static void main (String[] args) {
//fluxo de repetiÃ§Ã£o usando while 
int i = 3; // a variavel i recebe o valor trÃªs
do {
System.out.println (i) ; imprima val0or de i
i ++; //incremento da variavel i 
}
while (i < 5) { // enquanto i for menor que 5
} // fim do mÃ©todo 
  } // fim da classe da classe



class FOR{
 public static void main (String[] args) {
 for (i = 0; i < 5 ; i++) { // ordem crescente de 0 Ã  5
System.out.println (i) ; 
}
} // exibirÃ¡ o resultado da variavel i de 0 Ã  5 , tal como 0,1,2,3,4.

